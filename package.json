{
  "name": "animpath",
  "version": "1.0.3",
  "description": "Injectable animated stroke-based SVG library",
  "type": "module",
  "main": "./dist/animpath.umd.js",
  "unpkg": "./dist/animpath.umd.js",
  "module": "./dist/animpath.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/animpath.js",
      "require": "./dist/animpath.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --project tsconfig.build.json",
    "dev": "vite build --watch",
    "demo": "vite --config vite.demo.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,json}\""
  },
  "keywords": [
    "svg",
    "animation",
    "stroke",
    "drawing",
    "vivus",
    "icons",
    "lucide",
    "heroicons",
    "fontawesome",
    "iconify",
    "ionicons",
    "carbon",
    "phosphor",
    "material-icons"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.3.0",
    "highlight.js": "^11.11.1",
    "jsdom": "^28.1.0",
    "lucide": "^0.469.0",
    "perfect-scrollbar": "^1.5.6",
    "prettier": "^3.8.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^6.0.1",
    "vitest": "^2.1.6"
  }
}